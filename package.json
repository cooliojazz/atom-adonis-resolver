{
    "name": "adonis-resolver",
    "version": "0.2.0",
    "description": "Atom plugin to jump to an Adonis module's file from its use/make statement",
    "main": "index.js",
    "license": "MIT",
    "activationEvents": [
        "adonis-resolver:open-selected-dependencies"
    ],
    "scripts": {},
    "author": {
        "name": "Ricky Talbot",
        "email": "rickytalbot@hotmail.com"
    },
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cooliojazz/atom-adonis-resolver"
    },
    "dependencies": {
        "astw": "^1.2.0",
        "clone": "^0.1.17",
        "esprima": "^4.0.1",
        "file-position": "0.0.0",
        "static-eval": "^0.2.3"
    },
    "devDependencies": {},
    "package-deps": [
        "hyperclick"
    ],
    "providedServices": {
        "hyperclick.provider": {
            "versions": {
                "0.0.0": "provideAdonisHyperclick"
            }
        }
    },
    "keywords": [
        "adonis",
        "atom",
        "require",
        "plugin",
        "resolve",
        "file",
        "navigation"
    ],
    "homepage": "https://github.com/cooliojazz/atom-adonis-resolver",
    "bugs": {
        "url": "https://github.com/cooliojazz/atom-adonis-resolver/issues"
    }
